<!--index.wxml-->
<view class="container">
  <view>
    <a class="weui-btn weui-btn_primary" bindtap="main01">开始治疗</a>
    <view>{{currentText}}</view>
    <view class="main_img">
      <image src="/images/index.jpg" 
      bindtouchstart="touchStart" bindtouchend="touchEnd"></image>
    </view>
    <navigator url="../history/hindex">
      <a class="weui-btn weui-btn_disabled weui-btn_primary">历史结果</a>
    </navigator>
  </view>
  <view style="height:100px;"></view>

  <a class="weui-btn weui-btn_primary" bindtap="beginRecoder">开始录音</a>
  <a class="weui-btn weui-btn_primary" bindtap="endRecoder">结束录音</a>
  <!-- <view class="weui-btn weui-btn_primary" bindtouchstart="touchStart" bindtouchend="touchEnd">说话</view> -->


</view>
